        <record model="ir.ui.view" id="view_crm_meeting_form_inherited">
            <field name="name">crm.meeting.form.inherited</field>
            <field name="model">crm.meeting</field>
            <field name="inherit_id" ref="base_calendar.view_crm_meeting_form"/>
            <field name="arch" type="xml">
            <field name="name" position="after">
                <h4>
                    <field name="meeting_type" />
                </h4>
            </field>
            
			 <xpath expr="//field[@name='date']" position="before">
                <field name="schedule_appointment"/>
            </xpath>
            
			<xpath expr="//field[@name='date']" position="attributes">
                <attribute name="string">Actural Appointment Date</attribute>
            </xpath>
            
            <xpath expr="//form/sheet/notebook/page[1]/group/group[2]" position="after">
                <field name="appointment_outcome" colspan="4"/>
                <field name="reason" colspan="4"/>
            </xpath>
            <xpath expr="//field[@name='name']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            </field>
        </record>
        
