<?xml version="1.0"?>
<openerp>
    <data>
		<!-- CRM Lead Form View  -->
        <record id="view_sun_crm_lead_form" model="ir.ui.view">
			<field name="name">crm.lead.form</field>
			<field name="model">crm.lead</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="crm.crm_case_form_view_leads"/>
			<field name="arch" type="xml">
						
				<xpath expr="//button[@string='Schedule/Log Call']" position="replace">
                        <button string="Schedule/Log Call" 
                                name="%(crm.opportunity2phonecall_act)d"
                        		states="open"
                                type="action"/>
				</xpath>

				<xpath expr="///button[@string='Schedule/Log Call']" position="after">
					<button string="Meeting"
                                name="action_makeMeeting"
                                type="object"
                                context="{'search_default_attendee_id': active_id, 'default_attendee_id' : active_id}"/>
				</xpath>
				
				<xpath expr="//button[@string='Cancel Case']" position="attributes">
					<attribute name="string">Disqualify Lead</attribute>
				</xpath>
				
				<xpath expr="//field[@name='contact_name']" position="attributes">
					<attribute name="required">1</attribute>
					<attribute name="Placeholder">First Name</attribute>
				</xpath>
				
				<field name="contact_name" position="after">
				    <field name="last_name" placeholder="Last Name" required="1"/>
				</field>
				
				<xpath expr="//field[@name='street']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='street2']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='city']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='state_id']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='zip']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='country_id']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='phone']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='email_from']" position="attributes">
					<attribute name="required">1</attribute>
				</xpath>
				
				<xpath expr="//field[@name='mobile']" position="attributes">
					<attribute name="string">Cell</attribute>
				</xpath>
				
				<xpath expr="//page[@string='Internal Notes']" position="attributes">
					<attribute name="invisible">1</attribute>
				</xpath>
				
				<field name="user_id" position="before">
    				<field name="ahj" selection="1"/>
    				<field name="see_lead_all_note" invisible="1" />
				</field>
				<field name="section_id" position="after">
				    <button name="salesteam_send_email" type="object" string="Send Mail" attrs="{'invisible': ['|', ('section_id','=',False), ('user_id', '=', False)]}" />
				</field>
				<xpath expr="//form/sheet/notebook/page[@string='Internal Notes']" position="after">
				    <page string="Home Info">
		                <group col="4" colspan="4">
              				<field name="home" required="1"/>
              				<group col="10" colspan="10" attrs="{'invisible':[('home','!=','own')]}">
		                              <field name="time_own_year" />
		                              <span class="oe_grey"> Year </span><newline/>
		                              <span class="oe_grey">  </span>
		                              <field name="time_own_month"  nolabel="1"/>
		                              <span class="oe_grey"> Month </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                    	</group>
		                    <field name="quote" />
		                    <group col="4" colspan="4" attrs="{'invisible':[('quote','=',False)]}">
		                        <field name="quote_info" />
		                    </group>
		                    <field name="heat_home" />
		                    <field name="pool"/>
		                    <field name="spent_money"/>
		                    <field name="equity"/>
		                    <field name="home_sq_foot" /><newline/>
		                       <field name="roof_type" />
		                       <field name="estimate_shade" />
		                       <field name="tilt" />
		                       <field name="azimuth" />
		                          <group col="10" colspan="10">
		                              <field name="age_house_year" />
		                              <span class="oe_grey"> Year </span><newline/>
		                              <span class="oe_grey">  </span>
		                              <field name="age_house_month"  nolabel="1"/>
		                              <span class="oe_grey"> Month </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                    	</group>
		                </group>
		                <field name="see_lead_home_note" invisible="1" />
		                <label for="home_note"/>
		                <field name="home_note" colspan="4" />
		                <button name="view_lead_home_notes" string="Viewing This Tab Notes" type="object" class="oe_link"/>
		                <group>
		                <button name="view_lead_all_tab_notes" string="Viewing All Tab Notes" type="object" class="oe_link"/>
		                </group>
		            </page>
					<page string="Electricity Info">
						<group col="4" colspan="4">
						    <field name="utility_company_id" on_change="onchange_utility_company_id(utility_company_id)"  required="1"/>
						    <field name="required_document"/>
		                    <field name="acc_no" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <field name="meter_no" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <field name="bill_summer" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <field name="rate_plan" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <field name="bill_winter" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <label for="bill_average" attrs="{'invisible':[('required_document','=',False)]}"/>
		                    <div attrs="{'invisible':[('required_document','=',False)]}">
			                    <field name="bill_average"/>
			                    <span class="oe_grey"> of </span>
			                    <field name="bill_month" nolabel="1" readonly="1"/>
		                    </div>
		                    <field name="bill_total" attrs="{'invisible':[('required_document','=',False)]}"/>
						</group>
						<field name="see_lead_electricity_note" invisible="1" />
                        <label for="electricity_note"/>
                        <field name="electricity_note" colspan="4" />
                        <button name="view_lead_electricity_notes" string="Viewing This Tab Notes" type="object" class="oe_link"/>
                        <group>
                        <button name="view_lead_all_tab_notes" string="Viewing All Tab Notes" type="object" class="oe_link"/>
                        </group>
					</page>	
					<page string="Marketing Data">
                        <group col="4" colspan="4">
                            <field name="lead_source" />
                            <field name="level_of_lead" widget="selection"/>
                    <!--
							 <field name="schedule_appointment" />
                            <field name="outcome_appointment" />
																			-->
                    <!--    <field name="marketing_note" colspan="4"/>-->
                            <field name="qualified" />
                            <field name="annual_income" attrs="{'invisible':[('qualified','=',False)]}"/>
                            <field name="tax_liability" attrs="{'invisible':[('qualified','=',False)]}"/>
                            <field name="credit_source" attrs="{'invisible':[('qualified','=',False)]}"/>
                            <field name="property_tax" attrs="{'invisible':[('qualified','=',False)]}"/>
                        </group>
                        <field name="see_lead_marketing_note" invisible="1" />
                        <separator string="Appointments" colspan="4"/>
                        <field name="appointment_ids" mode="tree" nolabel="1" colspan="4"/>
                        <label for="marketing_note"/>
                        <field name="marketing_note" colspan="4" />
                        <button name="view_lead_marketing_notes" string="Viewing This Tab Notes" type="object" class="oe_link"/>
                        <group>
                        <button name="view_lead_all_tab_notes" string="Viewing All Tab Notes" type="object" class="oe_link"/>
                        </group>
                    </page>
                    <page string="Accounting Data">
                        <group col="4" colspan="4">
                            <field name="type_of_sale" />
                            <field name="deadline" />
                            <field name="deposit" />
                            <field name="federal_tax" />
                            <field name="property_tax" />
                            <field name="mortgage" />
                            <field name="bankruptcy" />
                            <field name="rate_credit" />
                        </group>
                        <field name="see_lead_accounting_note" invisible="1" />
                        <label for="accounting_note"/>
                        <field name="accounting_note" colspan="4" />
                        <button name="view_lead_accounting_notes" string="Viewing This Tab Notes" type="object" class="oe_link" />
                        <newline/>
                        <group>
                        <button name="view_lead_all_tab_notes" string="Viewing All Tab Notes" type="object" class="oe_link" />
                        </group>
                    </page>
                    <page string="System Data">
                        <group col="4" colspan="4">
	                        <field name="type_of_module" />
	                        <field name="inverter" />
	                        <field name="main_ele_serviece_panel" />
                        </group>
                        <field name="see_lead_system_note" invisible="1" />
                        <label for="system_note"/>
                        <field name="system_note" colspan="4" />
                        <button name="view_lead_system_notes" string="Viewing This Tab Notes" type="object" class="oe_link" colspan="4"/>
                        <group>
                        <button name="view_lead_all_tab_notes" string="Viewing All Tab Notes" type="object" class="oe_link" colspan="4"/>
                        </group>
                    </page>
                    <page string="Attachments">
                        <field name="attachment_ids" nolabel="1" colspan="4"/>
                    </page>
                    <page string="Viewing Home Notes" attrs="{'invisible':[('see_lead_home_note','=',False)]}">
                        <field name="crm_lead_home_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
                    <page string="Internal Notes">
                            <field name="description"/>
                        </page>
                    <page string="Viewing Electricity Notes" attrs="{'invisible':[('see_lead_electricity_note','=',False)]}">
                        <field name="crm_lead_electricity_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
                    <page string="Viewing Marketing Notes" attrs="{'invisible':[('see_lead_marketing_note','=',False)]}">
                        <field name="crm_lead_marketing_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
                    <page string="Viewing Accounting Notes" attrs="{'invisible':[('see_lead_accounting_note','=',False)]}">
                        <field name="crm_lead_accounting_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
                    <page string="Viewing System Notes" attrs="{'invisible':[('see_lead_system_note','=',False)]}">
                        <field name="crm_lead_system_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
                    <page string="Viewing All Notes" attrs="{'invisible':[('see_lead_all_note','=',False)]}">
                        <field name="crm_lead_all_tabs_note_ids" readonly="1" >
                            <tree>
                                <field name="date" />
                                <field name="notes" />
                            </tree>
                            <form>
                                <group>
                                    <field name="date" />
                                    <field name="notes" />
                                </group>
                            </form>
                        </field>
                        <button name="exit_notes" string="Exit Notes" type="object" class="oe_link"/>
                    </page>
				</xpath>

			</field>
		</record>
			
	<!-- Opportunities Form View -->
		<record id="view_sun_crm_opp_form" model="ir.ui.view">
		<field name="name">crm.opp.form</field>
		<field name="model">crm.lead</field>
		<field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
		<field name="arch" type="xml">

		<xpath expr="//form/header/button[2][@string='Mark Lost']" position="after">
			<button name="type_change" string="Convert to Lead" type="object" states="draft,open,pending" help="Convert to Lead" />
		</xpath>
		
		<xpath expr="/form/sheet/div[1]/button[1][@string='Schedule/Log Call']" position="attributes">
                <attribute name="invisible">1</attribute>
        </xpath>
        
        <xpath expr="//form/sheet/div[2]/h2" position="attributes">
                <attribute name="invisible">1</attribute>
        </xpath>
		<xpath expr="//field[@name='categ_ids']" position="after">
				<field name="responsible_user" attrs="{'invisible':[('stage_id','=','Appointment Set')]}"/>
        </xpath>

        <xpath expr="//field[@name='mobile']" position="attributes">
				<attribute name="string">Cell</attribute>
		</xpath>

		<xpath expr="//notebook/page[@string='Lead']" position="after">
			<page string="Home Info">
				<group col="4" colspan="4">
              		<field name="home" required="1"/>
              		<group col="10" colspan="10" attrs="{'invisible':[('home','!=','own')]}">
		                              <field name="time_own_year" />
		                              <span class="oe_grey"> Year </span><newline/>
		                              <span class="oe_grey">  </span>
		                              <field name="time_own_month"  nolabel="1"/>
		                              <span class="oe_grey"> Month </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                              <span class="oe_grey">  </span>
		                    	</group>
                  	<field name="quote" />
                  	<group col="4" colspan="4" attrs="{'invisible':[('quote','=',False)]}">
					    <field name="quote_info"/>
					</group>
					<field name="heat_home" />
					<field name="home_sq_foot" /><newline/>
                   </group>
				<group col="4" colspan="4">
                       <field name="roof_type"/>
                       <field name="estimate_shade"/>
                       <field name="tilt"/>
                       <field name="azimuth"/>
                          <group col="10" colspan="10">
                              <field name="age_house_year"/>
                              <span class="oe_grey"> Year </span><newline/>
                              <span class="oe_grey">  </span>
                              <field name="age_house_month" nolabel="1"/>
                              <span class="oe_grey"> Month </span>
                              <span class="oe_grey">  </span>
                              <span class="oe_grey">  </span>
                              <span class="oe_grey">  </span>
                              <span class="oe_grey">  </span>
                              <span class="oe_grey">  </span>
                              <span class="oe_grey">  </span>
					</group>
				</group>
			</page>
			<page string="Electricity Info">
                <group col="4" colspan="4">
                    <field name="utility_company_id" on_change="onchange_utility_company_id(utility_company_id)" required="1"/>
                    <field name="required_document"/>
                    <field name="acc_no" attrs="{'invisible':[('required_document','=',False)]}"/>
                    <field name="meter_no" attrs="{'invisible':[('required_document','=',False)]}"/>
                    <field name="bill_summer" attrs="{'invisible':[('required_document','=',False)]}"/>
                    <field name="rate_plan" attrs="{'invisible':[('required_document','=',False)]}"/>
		            <field name="bill_winter" attrs="{'invisible':[('required_document','=',False)]}"/>
                    <label for="bill_average" attrs="{'invisible':[('required_document','=',False)]}"/>
                    <div attrs="{'invisible':[('required_document','=',False)]}">
	                    <field name="bill_average"/>
	                    <span class="oe_grey"> of </span>
	                    <field name="bill_month" nolabel="1" readonly="1"/>
                    </div>
                    <field name="bill_total" attrs="{'invisible':[('required_document','=',False)]}"/>
                </group>                    
            </page> 
            <page string="Marketing Data">
                <group col="4" colspan="4">
                    <field name="lead_source"/>
                    <field name="level_of_lead" widget="selection"/>
                    <!--
					<field name="schedule_appointment" />
                    <field name="outcome_appointment" />
																				-->
                    <field name="qualified" />
                    <field name="annual_income" attrs="{'invisible':[('qualified','=',False)]}"/>
                    <field name="tax_liability" attrs="{'invisible':[('qualified','=',False)]}"/>
                    <field name="credit_source" attrs="{'invisible':[('qualified','=',False)]}"/>
                    <field name="property_tax" attrs="{'invisible':[('qualified','=',False)]}"/>
                    <separator string="Appointments" colspan="4"/>
                    <field name="appointment_ids" nolabel="1" colspan="4"/>
	               <field name="marketing_note" colspan="4"/>
                </group>
            </page>
            <page string="Accounting Data">
                <group col="4" colspan="4">
                    <field name="type_of_sale" />
                    <field name="deadline" />
                    <field name="deposit" />
                    <field name="federal_tax" />
                    <field name="property_tax" />
                    <field name="mortgage" />
                    <field name="bankruptcy" />
                    <field name="rate_credit" />
                </group>
            </page>
            <page string="System Data">
                <group col="4" colspan="4">
                    <field name="type_of_module" />
                    <field name="inverter" />
                    <field name="main_ele_serviece_panel" />
                </group>
            </page>
            <page string="Attachments">
                <field name="attachment_ids" nolabel="1" colspan="4"/>
            </page>
		</xpath>
	</field>
	</record>
	
    
