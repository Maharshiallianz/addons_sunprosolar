<?xml version="1.0" encoding="utf-8"?>
<openerp>

    <data noupdate="1">
        <record id="crm_rule_personal_lead_portal" model="ir.rule">
	        <field name="name">Portal Leads</field>
	        <field ref="crm.model_crm_lead" name="model_id"/>
	        <field name="domain_force">[('partner_id','=',user.partner_id.id)]</field>
        	<field name="groups" eval="[(4, ref('portal.group_portal'))]"/>
		</record>
		
        <record id="sps_dashboard_portal" model="ir.rule">
	        <field name="name">SPS Dashboard Portal</field>
	        <field ref="model_sps_dashboard" name="model_id"/>
	        <field name="domain_force">[('partner_id','=',user.partner_id.id)]</field>
        	<field name="groups" eval="[(4, ref('portal.group_portal'))]"/>
		</record>
    </data>

</openerp>
